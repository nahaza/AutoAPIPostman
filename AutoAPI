											"pm.environment.set(\"countItem\", countItem)\r",
											"var queryForSearch = \"СарафанЗадание40\"\r",
											"pm.environment.set(\"queryForSearch\", queryForSearch);\r",
											"itemName_arr = []\r",
											"itemId_arr = []\r",
											"itemDescription_arr = []\r",
											"itemPrice_arr = []\r",
											"pm.environment.set(\"Name_arr\", itemName_arr);\r",
											"pm.environment.set(\"Id_arr\", itemId_arr);\r",
											"pm.environment.set(\"Description_arr\", itemDescription_arr);\r",
											"pm.environment.set(\"Price_arr\", itemPrice_arr);\r",
											"pm.environment.set(\"SectionForDresses\", \"Платья\");\r",
											"\r",
											"for(i = 0; i < countItem; i++)\r",
											"{\r",
											"    const echoPostRequestCreateItem = {\r",
											"    url: 'http://shop.bugred.ru/api/items/create/',\r",
											"    method: 'POST',\r",
											"    header: 'Content-Type:application/json',\r",
											"    body: {\r",
											"    mode: 'raw',\r",
											"    raw: JSON.stringify({\r",
											"    \"name\": queryForSearch + Math.floor(Math.random() * 1000),\r",
											"    \"section\": pm.environment.get(\"SectionForDresses\"),\r",
											"    \"description\": queryForSearch + \"Dolce & Gabbana, артикул\" + Math.floor(Math.random() * 1000),\r",
											"    \"price\": Math.floor(Math.random() * 10000)})\r",
											"  }\r",
											"};\r",
											"\r",
											"   \r",
											"pm.sendRequest(echoPostRequestCreateItem, function (err, res) {\r",
											"     x = itemId_arr.push(res.json().result.id);\r",
											"     y = itemName_arr.push(res.json().result.name);\r",
											"     z = itemDescription_arr.push(res.json().result.description);\r",
											"     a = itemPrice_arr.push(res.json().result.price);     \r",
											"     console.log(itemName_arr);\r",
											"     console.log(itemId_arr);\r",
											"     console.log(itemDescription_arr);\r",
											"     console.log(itemPrice_arr);\r",
